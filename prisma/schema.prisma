// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id        Int      @id @default(autoincrement())
  name      String  
  elo       Int      @default(1000)
  login     String   @unique
  rank      Int      @default(0)
}

